#!/usr/bin/env bash

export EDITOR='vim'
export PATH=$HOME/bin:$PATH
if [ $(uname) = "Darwin" ]; then
  export PATH=$( gfind $HOME/bin -type d -name bin -printf "%p:" )$PATH
else
  export PATH=$( find $HOME/bin -type d -name bin -printf "%p:" )$PATH
fi
